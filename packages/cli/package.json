{
  "name": "@ribble-ui/cli",
  "version": "0.2.0",
  "description": "Interactive CLI for Ribble UI - scaffolding, theming, and project management",
  "bin": {
    "ribble": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "degit": "^2.8.4",
    "execa": "^8.0.1",
    "prompts": "^2.4.2",
    "picocolors": "^1.0.0",
    "chokidar": "^4.0.3"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "ribble-ui",
    "cli",
    "scaffolding",
    "web-components",
    "design-system"
  ],
  "license": "MIT"
}
